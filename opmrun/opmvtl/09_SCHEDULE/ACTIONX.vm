-- ---------------------------------------------------------------------------------------------------------------------------------
-- GROUP PHASE-1 AUTOMATIC PHASING
-- ---------------------------------------------------------------------------------------------------------------------------------
ACTIONX
        PHASE-1         1                                                      /
        GGPR  'FIELD' < 600E3 AND                                              /
        YEAR          > 2013  AND                                              /
        GMWDT 'FIELD' = 12                                                     /
/
       
--  WELL PRODUCTION STATUS                                                       
--                                                                              
--  WELL    WELL   --LOCATION--  COMPLETION                                     
--  NAME    STAT     I   J    K  FIRST LAST                                     
WELOPEN                                                                         
'GP*'       AUTO                                                               /
/   
                                                                            
-- WELL DRILLING QUEUE FOR PRIORITY DRILLING QUEUE                               
--                                                                              
-- WELL   PRIOR DRILL                                                           
-- NAME   VALUE UNIT                                                            
WDRILPRI                                                               
'GP01   '  1   1                                                               /
'GP02   '  1   1                                                               /
'GP03   '  1   1                                                               /
'GP04   '  1   1                                                               /
'GP05   '  1   1                                                               /
/

ENDACTIO
-- ---------------------------------------------------------------------------------------------------------------------------------
-- GROUP PHASE-2 AUTOMATIC PHASING
-- ---------------------------------------------------------------------------------------------------------------------------------
ACTIONX 
        PHASE-2         1                                                      /
        GGPR  'FIELD' < 600E3 AND                                              /
        YEAR          > 2013  AND                                              /
        GMWDT 'FIELD' = 17                                                     /
/
       
-- WELL PRODUCTION STATUS                                                       
--                                                                              
--  WELL    WELL   --LOCATION--  COMPLETION                                     
--  NAME    STAT     I   J    K  FIRST LAST                                     
WELOPEN                                                                         
'OP*'       AUTO                                                               /
/                                                                               

-- WELL DRILLING QUEUE FOR PRIORITY DRILLING QUEUE                               
--                                                                              
-- WELL   PRIOR DRILL                                                           
-- NAME   VALUE UNIT                                                            
WDRILPRI                                                               
'OP04'     2   2                                                               /
'OP05'     2   2                                                               /
'OP06'     2   2                                                               /
/
ENDACTIO

-- ---------------------------------------------------------------------------------------------------------------------------------
-- FIELD PHASE-3 AUTOMATIC COMPRESSION
-- ---------------------------------------------------------------------------------------------------------------------------------
ACTIONX 
        PHASE-3         1                                                      /
        GGPR  'FIELD' < 600E3 AND                                              /
        GMWDT 'FIELD' = 20                                                     /
/

--
-- INSTALL COMPRESSION AND RESET WELL THP AND BHPS
--
--  WELL    WELL   TARGET                                                       
--  NAME    TARG   VALUE                                                        
WELTARG                                                                         
'GP*     '  THP     450                                                        /
'GP*     '  BHP     300                                                        /
'OP*     '  THP     450                                                        /
'OP*     '  BHP     300                                                        /
/                                                                               

--
-- TEST AND OPEN ALL WELLS UNDER COMPRESSION CONSTRAINTS
--
--  WELL    TEST   CLOSE   NO.     START                                        
--  NAME    INTV   CHECK   CHECK   TIME                                        
WTEST                                                                    
'GP*     '  1.0    PE      1       3                                           /
'OP*     '  1.0    PE      1       3                                           /
/

ENDACTIO
